{
  "name": "@linear/sdk",
  "version": "1.0.0",
  "author": "Linear Orbit, Inc",
  "license": "MIT",
  "engines": {
    "node": ">=12.x",
    "yarn": "1.x"
  },
  "scripts": {
    "tsc": "npx tsc --noEmit",
    "build": "yarn build:generate && yarn build:clean && npx tsc",
    "build:clean": "npx rimraf dist",
    "build:generate": "yarn build:generate:clean && npx graphql-codegen --config codegen.yml",
    "build:generate:watch": "yarn build:generate --watch",
    "build:generate:clean": "npx rimraf src/_generated"
  },
  "dependencies": {
    "@graphql-codegen/introspection": "1.18.0",
    "@graphql-codegen/jsdoc": "1.17.8",
    "@graphql-codegen/typescript": "1.17.11",
    "@graphql-codegen/typescript-document-nodes": "1.17.8",
    "@graphql-codegen/typescript-operations": "1.17.9",
    "graphql": "15.4.0",
    "graphql-request": "3.3.0"
  }
}
