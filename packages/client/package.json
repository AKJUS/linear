{
  "name": "@linear/client",
  "version": "1.0.0",
  "author": "Linear Orbit, Inc",
  "license": "MIT",
  "main": "dist/index.js",
  "engines": {
    "node": ">=12.x",
    "yarn": "1.x"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev:client": "run-s dev:client:update build:client dev:client:push",
    "dev:client:push": "yalc push",
    "dev:client:update": "yalc update",
    "build:client": "yarn generate:sdk && yarn build:client:clean && npx tsc",
    "build:client:clean": "npx rimraf dist",
    "generate:sdk": "yarn generate:sdk:clean && npx graphql-codegen --config codegen.yml",
    "generate:sdk:watch": "yarn generate:sdk --watch",
    "generate:sdk:clean": "npx rimraf src/_generated"
  },
  "dependencies": {
    "@graphql-codegen/introspection": "1.18.0",
    "@graphql-codegen/typed-document-node": "1.17.10",
    "@graphql-codegen/typescript": "1.17.11",
    "@graphql-codegen/typescript-operations": "1.17.9",
    "@graphql-typed-document-node/core": "3.1.0",
    "@linear/sdk-plugin": "^1.0.0",
    "graphql": "15.4.0",
    "graphql-request": "3.3.0",
    "uuid": "8.3.1"
  },
  "devDependencies": {
    "dotenv": "8.2.0"
  }
}
