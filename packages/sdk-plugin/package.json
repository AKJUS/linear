{
  "name": "@linear/sdk-plugin",
  "version": "1.0.0",
  "author": "Linear Orbit, Inc",
  "license": "MIT",
  "main": "dist/index.js",
  "engines": {
    "node": ">=12.x",
    "yarn": "1.x"
  },
  "scripts": {
    "tsc": "npx tsc --noEmit",
    "build:plugin": "yarn build:clean && npx tsc",
    "build:watch": "yarn build --watch",
    "build:clean": "npx rimraf dist"
  },
  "dependencies": {
    "@graphql-codegen/plugin-helpers": "1.18.0",
    "@graphql-codegen/visitor-plugin-common": "1.17.16",
    "auto-bind": "4.0.0",
    "graphql": "15.4.0",
    "path": "0.12.7",
    "typescript": "4.0.5"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "testMatch": [
      "**/*.tests.ts?(x)"
    ],
    "transform": {
      "^.*\\.tsx?$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    }
  }
}
