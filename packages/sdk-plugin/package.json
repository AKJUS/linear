  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": ">=12.x",
    "yarn": "1.x"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev:plugin": "yarn dev:plugin:sdk",
    "dev:plugin:sdk": "NODE_ENV=development run-s dev:plugin:sdk:update build:plugin:sdk dev:plugin:sdk:push",
    "dev:plugin:sdk:push": "yalc push",
    "dev:plugin:sdk:update": "yalc update",
    "build:plugin": "yarn build:plugin:sdk",
    "build:plugin:sdk": "yarn build:plugin:sdk:clean && npx tsc",
    "build:plugin:sdk:clean": "npx rimraf dist"
  },
  "dependencies": {
    "@graphql-codegen/plugin-helpers": "1.18.1",
    "@graphql-codegen/visitor-plugin-common": "1.17.17",
    "@graphql-tools/graphql-file-loader": "6.2.5",
    "@graphql-tools/load": "6.2.5",
    "@linear/common": "^1.0.0",
    "auto-bind": "4.0.0",
    "graphql": "15.4.0",
    "path": "0.12.7",
    "typescript": "4.0.5"
  }
}
